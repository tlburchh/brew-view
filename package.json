{
  "name": "bruvue_web_assignment",
  "version": "1.0.0",
  "description": "this is the backend of bruvue",
  "keywords": [
    "react"
  ],
  "main": "client/pages/index.js",
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^2.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.11",
    "morgan": "^1.9.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^3.6.1",
    "nodemon": "^1.11.0",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.0"
  },
  "scripts": {
    "start-local": "webpack-dev-server --env.local --progress --hot --open",
    "test": "react-scripts test --env=jsdom",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node server/scripts/seedDB.js && node server/scripts/seedAssociations.js",
    "seed2": "node server/scripts/seedAssociations.js",
    "install": "cd client && yarn install"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.x",
    "yarn": "1.10.1"
  }
}
